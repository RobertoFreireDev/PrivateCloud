@page "/databases"

<MudContainer MaxWidth="MaxWidth.Medium" Class="mt-4">
    <JsEditor CodeContent="@content"
              Style="height:500px; width:100%;"
              ContentChanged="@OnContentChanged"
              SelectedContentChanged="@OnSelectedContentChanged" />
    <MudPaper Class="pa-4">
        <MudButton Variant="Variant.Filled" Color="Color.Primary" OnClick="ExecuteFunction">
            Execute
        </MudButton>
        <DynamicTable JsonString="@ConsoleText"></DynamicTable>
    </MudPaper>
</MudContainer>